// Function to Play Audio via DAC
void playAudioDAC(const uint8_t* audioData, size_t dataSize) {
    for (size_t i = 0; i < dataSize; i++) {
        dacWrite(DAC_CHANNEL, audioData[i]);  // Output to ESP32 DAC
        delayMicroseconds(62);  // Approximate 16kHz sample rate
    }
}

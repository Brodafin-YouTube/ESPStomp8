//************************************************************ Begin Setup *******************************************************************
void setup() {
    Wire.begin(); //  **************Set Up OLED Display ************
    display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
    display.clearDisplay();
    display.setTextColor(WHITE); 
    display.setTextSize(2);  // Mid font
    display.setCursor(5, 15);  // Position left digit
    display.println("ESP STOMP8"); display.display(); delay(3000);
    display.clearDisplay();
    //display.setTextSize(3);  // Larger font
    showAudioFileCounts();
    display.display(); // ********** End OLED Display Setup ********** 
    WiFi.disconnect(true); WiFi.mode(WIFI_OFF); esp_bt_controller_disable(); //WiFi & Bluetooth Off
    pinMode(BUTTON1_PIN, INPUT_PULLUP);// Configure buttons with internal pull-up resistors
    pinMode(BUTTON2_PIN, INPUT_PULLUP);
    pinMode(BUTTON3_PIN, INPUT_PULLUP);
    pinMode(BUTTON4_PIN, INPUT_PULLUP);
    // Attach interrupts to buttons 3 and 4
    attachInterrupt(BUTTON3_PIN, button3Pressed, FALLING);  // Third button interrupt. Left Sound Select     
    attachInterrupt(BUTTON4_PIN, button4Pressed, FALLING);  // Fourth button interrupt. Right Sound Select 
    playAudioDAC(audioFiles[counter1], audioSizes[counter1]); // Clear Any Pops before beginning
}

// MAIN LOOP ----------------------------------
void loop() {
     // Check Button 1
    bool currentButtonState1 = digitalRead(BUTTON1_PIN);
    if (currentButtonState1 == LOW && lastButtonState1 == HIGH) {
        lastDebounceTime1 = millis();
        pressStartTime1 = millis();
        button1Handled = false;
    }
    if (currentButtonState1 == LOW && !button1Handled) {
        if (millis() - pressStartTime1 > DEBOUNCE_DELAY) {  
            playAudioDAC(audioFiles[counter1], audioSizes[counter1]); 
            button1Handled = true;  
        }
    }
    if (currentButtonState1 == HIGH && lastButtonState1 == LOW) {
        button1Handled = false;  
    }
    lastButtonState1 = currentButtonState1;  

    // Check Button 2
    bool currentButtonState2 = digitalRead(BUTTON2_PIN);
    if (currentButtonState2 == LOW && lastButtonState2 == HIGH) {
        lastDebounceTime2 = millis();
        pressStartTime2 = millis();
        button2Handled = false;
    }
    if (currentButtonState2 == LOW && !button2Handled) {
        if (millis() - pressStartTime2 > DEBOUNCE_DELAY) {  
            playAudioDAC(audioFiles[counter2], audioSizes[counter2]); 
            button2Handled = true;  
        }
    }
    if (currentButtonState2 == HIGH && lastButtonState2 == LOW) {
        button2Handled = false;  
    }
    lastButtonState2 = currentButtonState2;  
    if (button3Flag) {
     showAudioFileCounts();
      delay(250);
      button3Flag = false;
    }
    if (button4Flag) {
     showAudioFileCounts();
      delay(250);
      button4Flag = false;
    }
}
// END MAIN LOOP ----------------------------------

